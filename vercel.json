{
  "$schema": "https://openapi.vercel.sh/vercel.json",

  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "header", "key": "accept", "value": "text/html" }
      ],
      "destination": "/index.html"
    }
  ]
}
